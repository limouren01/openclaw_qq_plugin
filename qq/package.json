{
  "name": "@openclaw/qq",
  "version": "2026.2.10",
  "description": "OpenClaw QQ channel plugin via Napcat",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "openclaw": {
    "extensions": [
      "./src/index.ts"
    ],
    "channel": {
      "id": "qq",
      "label": "QQ",
      "selectionLabel": "QQ (Napcat)",
      "docsPath": "/channels/qq",
      "docsLabel": "qq",
      "blurb": "QQ channel via Napcat; requires running Napcat instance.",
      "order": 50
    },
    "install": {
      "localPath": "extensions/qq",
      "defaultChoice": "local"
    }
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "openclaw": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10"
  }
}